@model IEnumerable<Core_1.ViewModels.EmployeeViewModel>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>Index</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.SurName</td>
                <td>@item.Patronymic</td>
                <td>@item.Age</td>
                <td>@Html.ActionLink("Детали", "Employee", new { id = item.Id })</td>
            </tr>
        }
    </table>
    @Html.ActionLink("На главную", "Index", "Employee")
</body>
</html>