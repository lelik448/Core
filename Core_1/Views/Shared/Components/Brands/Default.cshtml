@model IEnumerable<Core.ViewModels.BrandViewModel>


    <div class="brands_products">
        <!--brands_products-->
        <h2>Brands</h2>
        <div class="brands-name">
            <ul class="nav nav-pills nav-stacked">
                @foreach (var Brand in Model)
                {
                    <li><a href="#"> <span class="pull-right">(@Brand.BrandsCount)</span>@Brand.Name</a></li>
                   
                }

                </ul>
        </div>
    </div><!--/brands_products-->

    